.container
  .carousel.slide.col-md5{:id => "carousel",
                          :data => {:ride => "carousel"}}
    %ol.carousel-indicators
      %li.active{:data => {:target => "#carousel",
                           :slide => {:to => "0"}}}
      %li{:data => {:target => "#carousel", 
                    :slide => {:to => "1"}}}
    .carousel-inner
      .item.active
        %img{:src => "http://imagecache.artistrising.com/artwork/lrg//4/490/YZC6000A.jpg",
             :alt => "..."}
        .carousel-caption
      .item
        %img{:src => "http://dda604.com/wp-content/uploads/2011/02/vancouver-skyline-400x200.jpg", :alt => "..."}
    %a.left.carousel-control{:href => "#carousel",
                             :data => {:slide => "prev"}}
      %span.glyphicon.glyphicon-chevron-left
    %a.right.carousel-control{:href => "#carousel",
                              :data => {:slide => "next"}}
      %span.glyphicon.glyphicon-chevron-right
